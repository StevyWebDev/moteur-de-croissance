<form method="post">
    <div class="bloc-input">
        <label class="label phone-label" for="phone">Téléphone</label>
        <input type="tel" name="phone" id="phone" placeholder="Téléphone">
    </div> 
    
    <div class="bloc-button">
        <div class="bloc-login">
            <button class="button button-login" type="submit">Envoyer</button>
        </div>
        <div class="bloc-inscription">
            <button class="button button-inscription" onclick="deleteForm()">Retour</button>
        </div>
    </div>
</form>
